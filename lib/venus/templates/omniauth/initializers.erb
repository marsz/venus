Rails.application.config.middleware.use OmniAuth::Builder do
<%- @providers.each do |provider, | %>
  provider :<%= provider %>, Setting.omniauth.<%= provider %>.app_id, Setting.omniauth.<%= provider %>.app_secret, :scope => Setting.omniauth.<%= provider %>.scope
<%- end %>
end

OmniAuth.config.path_prefix = '/<%= @devise_table %>/auth'
